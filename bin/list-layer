#!/usr/bin/env bash 

if [ "$#" -ne 1 ]; then 
  echo "usage: $0 image-name"
  exit 1
fi

IMAGENAME=$1
LAYERS=$(manifest ${IMAGENAME} | jq ".[0].Layers")

echo $LAYERS

read L

LAYER=$(manifest ${IMAGENAME} | jq '.[0].Layers['${L}']')

echo $LAYER

temp="${LAYER%\"}"
temp="${temp#\"}"
LAYER=${temp}

tar --list --verbose -f ./images/${IMAGENAME}/${LAYER}
